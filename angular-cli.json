{
  "project": {
    "version": "1.1.0",
    "name": "lukeify"
  },
  "apps": [
    {
      "root": "angular",
      "outDir": "dist",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "scripts": [],
      "styles": [
        "assets/styles/lukeify.scss"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "defaults": {
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "styleExt": "scss",
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    },
    "build": {
      "showCircularDependencies": true
    }
  }
}
